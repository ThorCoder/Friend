.minfo{
  padding: 30rpx;
  display: flex;
  justify-content:center;
  align-items:center;
  flex-direction:column;
  font-size: 14pt;
}
.minfo .avatar{
  width:120rpx;
  height: 120rpx;
  border-radius:100%;
  overflow: hidden;
}

.list .item{
  display: flex;
  height:100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ccc;
  align-items:center;
  padding: 0 20rpx;
}
.list .item.default .title{
  flex-grow:1;
}
.list .item.icon image{
  width:46rpx;
  height: 46rpx;
}